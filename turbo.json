{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.ts", "./tsup.config.ts"],
      "outputs": ["dist/**", "build/**", ".hydrogen/**"]
    },
    "dev": {
      "dependsOn": [
        "@shopify/cli-h2-test#build",
        "@shopify/h2-test-hydrogen-remix#build"
      ],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": [],
      "dependsOn": ["build"]
    },
    "@shopify/h2-test-hydrogen-remix#build": {
      "dependsOn": [
        "@shopify/h2-test-hydrogen#build",
        "@shopify/h2-test-remix-oxygen#build"
      ]
    },
    "demo-store#build": {
      "dependsOn": [
        "@shopify/cli-h2-test#build",
        "@shopify/h2-test-hydrogen-remix#build"
      ],
      "outputs": ["dist/**", ".hydrogen/**"]
    },
    "hello-world#build": {
      "dependsOn": [
        "@shopify/cli-h2-test#build",
        "@shopify/h2-test-hydrogen-remix#build"
      ],
      "outputs": ["dist/**"]
    }
  }
}
