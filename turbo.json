{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["@shopify/hydrogen-remix#build"],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": [],
      "dependsOn": ["build"]
    },
    "@shopify/hydrogen-remix#build": {
      "dependsOn": ["@shopify/hydrogen#build", "@remix-run/oxygen#build"]
    },
    "demo-store#build": {
      "dependsOn": ["@shopify/hydrogen-remix#build"]
    }
  }
}
